				$(this).find('.inner').hide();
				$(this).removeClass('animating');
			})
		})
		if (!$('#gremDrop').hasClass('animating')){
			$('#gremDrop').addClass('animating');
			$('#gremDrop').show().animate({width:'250px',left: '250px'},200,function(){
				$('#gremDrop .inner').show();
				$('#gremDrop').removeClass('animating');
			})
		}

