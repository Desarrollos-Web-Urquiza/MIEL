$(document).ready(function(){


	$('#header').mouseenter(function(){
		$('.dropable').each(function(){
			$(this).show().animate({width:'0px',left: '0px'},0,function(){
				$(this).find('.inner').hide();
				$(this).removeClass('animating');
			})
		})

	});

